<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              tools:context=".fragments.InsertDataFragment">


    <Button
            android:id="@+id/mInsertData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Insert Data"
            android:textSize="20sp"
            android:gravity="center_horizontal"
            android:layout_margin="20dp"/>

    <Button android:id="@+id/mDeleteData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Delete Data"
            android:textSize="20sp"
            android:gravity="center_horizontal"
            android:layout_margin="20dp"/>

    <Button android:id="@+id/mUpdateData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Update Data"
            android:textSize="20sp"
            android:gravity="center_horizontal"
            android:layout_margin="20dp"/>

    <Button android:id="@+id/mGetData"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Get Data"
            android:textSize="20sp"
            android:gravity="center_horizontal"
            android:layout_margin="20dp"/>


    <Button android:id="@+id/mGetDataList"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Get Data List"
            android:textSize="20sp"
            android:gravity="center_horizontal"
            android:layout_margin="20dp"/>

</LinearLayout>